type Cast {
	id: ID!
	status: Int
	cast_image_url: String!
	name: String!
}

extend type Query {
	countCasts: Int!
	castById(id:ID!):Cast
}

# Root
extend type Mutation {
	createCast(name: String!, cast_image_url: String!): Cast!
	updateCast(id: Int!, name: String!, cast_image_url: String!): Cast!
	softDeleteCastById(id: Int!): Cast!
	setActiveCastById(id: Int!): Cast!
}
