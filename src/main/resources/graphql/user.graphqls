scalar Long

type User {
	id: ID!
	status: ID!
	username: String!
	email: String!
	telephone: String!
	password: String!
}

type Query {
	findAllUsers: [User]!
	countUsers: Long!
	userById(id:ID!):User 
}

type Mutation {
	createUser(username: String!, email: String!, password: String!, telephone: String!): User!
}

